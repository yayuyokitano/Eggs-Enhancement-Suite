{
  "manifest_version": 3,
  "name": "Eggs Enhancement Suite",
  "version": "2.0.0",
  "permissions": [
		"webRequest"
  ],
  "background": {
    "service_worker": "background/main.ts",
    "type": "module"
  },
  "content_scripts": [{
    "matches": ["*://eggs.mu/*"],
    "js": ["content/main.tsx"]
  }],
	"action": {
		"default_icon": {
			"16": "icons/icon16.png",
			"19": "icons/icon19.png",
			"24": "icons/icon24.png",
			"32": "icons/icon32.png",
			"48": "icons/icon48.png",
			"64": "icons/icon64.png",
			"128": "icons/icon128.png"
		},
		"default_title": "Eggs Enhancement Suite",
		"default_popup": "App/popup/popup.html"
	},
	"icons": {
		"16": "icons/icon16.png",
		"19": "icons/icon19.png",
		"24": "icons/icon24.png",
		"32": "icons/icon32.png",
		"48": "icons/icon48.png",
		"64": "icons/icon64.png",
		"128": "icons/icon128.png"
	}
}