{
  "manifest_version": 3,
  "name": "Eggs Enhancement Suite",
  "version": "2.0.0",
  "permissions": [
    "webRequest",
    "webRequestBlocking"
  ],
  "background": {
    "service_worker": "background/main.ts",
    "type": "module"
  },
  "content_scripts": [{
    "matches": ["*://eggs.mu/*"],
    "js": ["content/main.tsx"]
  }]
}