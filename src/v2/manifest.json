{
	"manifest_version": 2,
	"name": "Eggs Enhancement Suite",
	"version": "2.0.0",
	"permissions": [
		"storage",
		"*://*.eggs.mu/*"
	],
	"content_scripts": [{
		"matches": ["*://eggs.mu/*"],
		"js": ["../content.tsx"],
		"all_frames": true,
		"run_at": "document_start"
	}],
	"browser_action": {
		"default_icon": {
			"16": "../icons/icon16.png",
			"19": "../icons/icon19.png",
			"24": "../icons/icon24.png",
			"32": "../icons/icon32.png",
			"48": "../icons/icon48.png",
			"64": "../icons/icon64.png",
			"128": "../icons/icon128.png"
		},
		"default_title": "Eggs Enhancement Suite",
		"default_popup": "../App/popup/popup.html"
	},
	"icons": {
		"16": "../icons/icon16.png",
		"19": "../icons/icon19.png",
		"24": "../icons/icon24.png",
		"32": "../icons/icon32.png",
		"48": "../icons/icon48.png",
		"64": "../icons/icon64.png",
		"128": "../icons/icon128.png"
	},
	"web_accessible_resources": ["../icons/icon128.png"],
	"browser_specific_settings": {
		"gecko": {
			"id": "maxmulhallbiseth@gmail.com"
		}
	}
}